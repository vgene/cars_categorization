# The train/test net protocol buffer definition
net: "part9/train_val.prototxt"
test_iter: 10
test_interval: 500
base_lr: 0.00001
lr_policy: "step"
gamma: 0.1
stepsize: 10000
display: 100
max_iter: 1000000
momentum: 0.9
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "part9/part9"
solver_mode: GPU
